{
    "plugins": [],
    "printWidth": 150,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "singleAttributePerLine": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "semi": true,
    "overrides": [
        {
            "files": "*.ts",
            "options": {
                "sqlKeywordCase": "upper",
                "sqlIndent": true,
                "sql-template-strings": {
                    "singleLine": false
                }
            }
        }
    ]
}
